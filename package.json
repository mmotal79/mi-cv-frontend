{
  "name": "mi-cv-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:backend": "node server.js",
    "dev": "concurrently \"npm run start:backend\" \"npm run start\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --fix",
    "test-network": "ping 192.168.1.105 && telnet 192.168.1.105 3001"
  },
  "proxy": "http://localhost:10000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "concurrently": "^8.2.2",
    "prettier": "^3.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "description": "Portfolio profesional de Miguel Mota con agente IA integrado",
  "keywords": [
    "portfolio",
    "react",
    "tailwindcss",
    "ai"
  ],
  "author": "Miguel Mota",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/mi-cv-frontend.git"
  }
}
